#commission_div table, #commission_en_div table,.inbound_commission_table {
	font-family:Arial, Helvetica, sans-serif;
	color:#666;
	font-size:12px;
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	border-radius:3px;
	width: 1220px;
	text-align: center;
}
#commission_div table tr th.category, #commission_en_div table tr th.category{
	width: 180px;
}
#commission_div table tr th.duration, #commission_en_div table tr th.duration{
	width: 50px;
}
#commission_div table tr td.top_category, #commission_en_div table tr td.top_category {
	text-align: left;
	padding-left: 12px;	
	color: red;
}
#commission_div table th, #commission_en_div table th,.commission_table_head>th {
	padding:5px 5px 5px 5px;
	border-bottom:1px dashed #58595b;
	background-color: #ededed;
	background: -webkit-gradient(linear, left top, left bottom, from(#ededed), to(#ebebeb));
	background: -moz-linear-gradient(top,  #ededed,  #ebebeb);
}

#commission_div table th:first-child, #commission_en_div table th:first-child, .inbound_commission_table tr th:first-child{
	text-align: left;
	padding-left:20px;
}

#commission_div table tr, #commission_en_div table tr, .inbound_commission_table tr td{
	text-align: center;
	padding-left:20px;
}
#commission_div table tr td:first-child, #commission_en_div table tr td:first-child, .inbound_commission_table tr td:first-child{
	text-align: left;
	padding-left:20px;
	border-left: 0;
}
#commission_div table tr td, #commission_en_div table tr td,.inbound_commission_table tbody tr td {
	padding:5px;
	border-top: 1px dashed #ffffff;
	border-bottom:1px dashed #58595b;
	border-left: 1px solid #e0e0e0;
	

}
