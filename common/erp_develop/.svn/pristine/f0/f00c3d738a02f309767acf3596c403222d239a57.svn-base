#commission_table tr.tour_row:nth-child(2n+1){
    background-color: #F2F6F8;
}

#commission_table tr.tour_row td.ignore_bg{
    background-color: #FFF;
}

.inbound_commission_table tbody tr:nth-child(2n+1){
    background-color: #F2F6F8;
}

.room_comm_wrapper{
    text-align: left;
    display: inline-block;
}

.room_comm_wrapper>label{
    display: inline-block;
    width: 50px;
}