                    if(item.value==''){
                        team_count++;
                    }else{
                        if($(item).attr("data-vals")==i) {
                            team_product_array[team_product_array.length-1].team_product_id = item.value;
                        }
                    }
                })
            }
        }
    }
    if(count+team_count==0){
        var yan=[];
        var teamYan=[];
        $("#productList").find("td").each(function (index, item) {
            if ($(item).attr("data-number") != undefined) {
                var number=$(item).attr("data-tims");
                yan.push(number);
            }
        })
        $("#productM").find("td").each(function (index, item) {
            if ($(item).attr("data-number") != undefined) {
                var number=$(item).attr("data-tims");
                teamYan.push(number);
            }
        })
        var nary=yan.sort();
        var teamNary=teamYan.sort();
        var counts=0;
        var team_counts=0;
        for(var i=0;i<yan.length;i++) {
            if (nary[i] == nary[i + 1]) {
                counts++;
            }
        }
        for(var i=0;i<teamYan.length;i++) {
            if (teamNary[i] == teamNary[i + 1]) {
                team_counts++;
            }
        }
        if(counts==0||team_counts==0){
            layer.load(2);
            $.ajax({
                type : "post",
                url : "/companyorder/addCompanyOrderProductAjax",
                data : {
                    company_order_number:(window.location.search).split("=")[1],
                    branch_product_array:JSON.stringify(branch_product_array),
                    team_product_array:JSON.stringify(team_product_array)
                },
                dataType : "json",
                success : function(result) {
                    if(result.code==200){
                        var list=result.data;
                        addRoute=[];
                        finalRoute=[];
                        productOther();
                    }else if(result.code!=200){
                        return false;
                    }

                }
            })
        }else{
            layer.msg("不能选择重复的团！请重新选择到达日期");
        }
    }

}
