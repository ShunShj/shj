/*财务收款弹出确定*/
function addFinancial(obj) {
    var sn=$("#SN").val();
    var payment_currency_id=$("#financialRMB").val();
    var financialTime=$("#financialTime").val();
    var financialType=$("#financialType").val();
    var financialPayment=$("#financialPayment").val();
    var payNumber=$("#payNumber").val();
    var supplier=$("#supplier").attr("data-names");
    var amount=$("#amount").val();
    var financialNumber=$("#financialNumber").val();
    var huidui=$("#huidui").val();
    var accountNumber=$("#accountNumber").val();
    var financialRemark=$("#financialRemark").val();
    $.ajax({
        type: "post",
        url: "/companyOrder/addReceivableInfoAjax",
        data:{
            sn_number:sn,
            payment_time:dates(financialTime),
            payment_type:financialType,
            payment_stage:financialPayment,
            payment_currency_id:payment_currency_id,
            payment_number:payNumber,
            supplier_name:supplier,
            payment_money:amount,
            receivable_number:financialNumber,
            exg_rate_gain:huidui,
            account_number:accountNumber,
            remark:financialRemark
        },
        dataType: "json",
        success: function (result) {
            if(result.code==200) {
                layer.load(2);
                layer.msg("添加成功！");
                financial();
                layer.closeAll();
            }else if(result.code!=200){
                return false;
            }
        }
    });
}
function dates(obj) {
    if(obj!=''){
        return Math.round(new Date(obj).getTime()/1000).toString();
    }else{
        return '';
    }
}
/*删除销售收款*/
function sales_del(obj,names) {
    var ids=$(obj).parents("tr").attr("data-del");
    financial_del(ids,names)
}
/*删除财务收款*/
function financial_del(obj,names) {

    layer.load(2);
    var receivable_info_id;
    if(names=='sales'){
        receivable_info_id=obj;
    }else{
        receivable_info_id=$(obj).attr("data-infoId");
    }
    $.ajax({
        type: "post",
        url: "/finance/updateFinanceApproveAjax",
        data:{
            finance_approve_id:receivable_info_id,
            status:0
        },
        dataType: "json",
        success: function (result) {
            if(result.code==200) {
                var list=result.data;
                layer.msg("删除成功！");
                if(names=='sales'){
                    receivable();
                }else{
                    financial();
                }
                layer.closeAll('loading');
            }else if(result.code!=200){
                return false;
            }
        }
    });
}
/*修改财务收款*/
function financialPayment(obj,stutas,names) {//receivable_info_id、选择状态、选择的名称pts/paid
    layer.load(2);
    var pts='';
    var paid='';
    names=='pts'?pts=stutas:paid=stutas;
    $.ajax({
        type: "post",
        url: "/finance/updateFinancialPaymentStatus",
        data:{
            receivable_info_id:obj,
            pts:pts,
            is_locked:paid
        },
        dataType: "json",
        success: function (result) {
            if(result.code==200) {
                var list=result.data;
                layer.msg("选择成功！")
                if(names=='paids'){
                    receivable();
                }
                layer.closeAll('loading');
            }else if(result.code!=200){
                return false;
            }
        }
    });
}

/*对应应收*/
function receivables() {
    $.ajax({
        type: "post",
        url: "/finance/getReceivableAjax",
        data:{
            order_number:(window.location.search).split("=")[1]
        },
        dataType: "json",
        success: function (result) {
            layer.load(2);
            if(result.code==200) {
                var list=result.data;
                var popSaleNumber=$("#popSaleNumber").html();
                laytpl(popSaleNumber).render(list, function(html) {
                    layer.open({
                        skin: 'my-skin'
                        , content: html
                        , type: 1
                        , title: '对应应收'
                        , area: ['700px', '550px']
                        , shadeClose: true,
                        scrollbar: false,
                        success: function () {
                            var number='';
                            var arrayNum=addNumber.split(",");
                            $("#allNumber").attr("disabled","true");
                            for(var y=0;y<arrayNum.length;y++){
                                $("#popSaleTr").find('input[type="checkbox"]').each(function(index, item) {
                                    if($(item).parent("td").attr("data-number")==arrayNum[y]){
                                        number=$(item).parent("td").attr("data-currency");
                                        document.getElementById("allNumber").disabled=false;
                                        item.checked=true;
                                    }else{
                                        if($(item).parent("td").attr("data-currency")!=number&&number!=''){
                                            item.disabled=true;
                                            $(item).parents("tr").css("background","#ddd");
                                        }
                                    }
                                });
                            }
                            form.on('checkbox(saleList)', function(data){
                                var sum=0;
                                var currencyName=$(data.elem).parent("td").attr("data-currency");
                                $("#popSaleTr").find('input[type="checkbox"]').each(function(index, item) {
                                    if(item.checked==true){
                                        document.getElementById("allNumber").disabled=false;
                                        sum++;
                                    }
                                    if(sum==0&&data.elem.checked==false){
                                        if($(item).parent("td").attr("data-currency")!=currencyName){
                                            item.disabled=data.elem.disabled;
                                            $(item).parents("tr").css("background","#fff");
                                        }
                                    }else{
                                        if($(item).parent("td").attr("data-currency")!=currencyName){
                                            item.disabled=true;
                                            $(item).parents("tr").css("background","#ddd");
                                        }
                                    }
                                });
                                form.render('checkbox');
                            });
                            form.render();
                        }
                    })
                });
                layer.closeAll('loading');
            }else if(result.code!=200){
                return false;
            }
        }
    });
}
