function financial() {
    $.ajax({
        type: "post",
        url: "/finance/getReceivableInfoAjax",
        data:{
            company_order_number:(window.location.search).split("=")[1],
            status:1,
    
        },
        dataType: "json",
        success: function (result) {
            layer.load(2);
            if(result.code==200) {
                var list=result.data;

                var arr=[];
                for(var i=0;i<list.length;i++){
                    list[i].children=[];
                    if(arr.length==0){
                        list[i].indexs=1;
                        arr.push(list[i].payment_number);
                    }else{
                        var index=0;
                        for(var j=0;j<arr.length;j++){
                            if(list[i].payment_number==arr[j]){
                                index++;
                            }
                        }
                        if(index==0){
                            list[i].indexs=1;
                            arr.push(list[i].payment_number);
                        }else{
                            list[i].indexs=2;
                        }
                    }
                    for(var k=0;k<list.length;k++){
                        if(list[i].payment_number==list[k].payment_number){
                            list[i].children.push(list[k]);
                        }
                    }
                    list[i].children.splice(0,1);
                }

                var financial=$("#financialTbody").html();
                laytpl(financial).render(list, function(html){
                    $("#financial").html(html);
                });
                form.render();
                layer.closeAll('loading');
            }else if(result.code!=200){
                return false;
            }
        }
    });
}
