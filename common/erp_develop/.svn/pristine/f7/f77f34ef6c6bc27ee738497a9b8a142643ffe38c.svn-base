        , btnAlign: 'c' //按钮居中
        , area: ['1200px', '490px']
        , shadeClose: true,
        scrollbar: false,
        success:function () {

            layer.closeAll('loading');
        }
    }, function () {
        layer.load(2);
        //selectRoute
        $("#team_M_table").find('td input[type="checkbox"]').each(function(index, item) {
            if (item.checked === true) {
                if($(item).parent("td").attr("data-selected")!='old'){
                    finalRoute.push({route_template_id:routeTemplate[index].route_template_id,route_template_number:routeTemplate[index].route_number,route_name:routeTemplate[index].route_name,route_type_id:routeTemplate[index].route_type_id,isNew:'1'});
                }
            }
        });
        for(var i=0;i<finalRoute.length;i++){
            var route_template_id=finalRoute[i].route_template_id;
            $.ajax({
                type : "post",
                async: false,
                url : "/product/selTeamProductbyRouteTemplateId",
                data : {
                    route_template_id:route_template_id
                },
                dataType : "json",
                success : function(result) {
                    if(result.code==200){
                        var list=result.data;
                        finalRoute[i].times=list;
                        form.render();
                    }else if(result.code!=200){
                        return false;
                    }
                }
            })
        }
        console.log(finalRoute)
        productM();
        form.render();
    })
}
