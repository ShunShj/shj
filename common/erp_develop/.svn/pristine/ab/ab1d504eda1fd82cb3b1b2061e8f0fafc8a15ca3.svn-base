                if($("#branch_product_number").val()=='' || tableId!=undefined){
                    addAllPop[i].price_currency_id=addAllPop[i].cost_currency_id;
                }

                for(var d=0;d<datas.company.length;d++){
                    if(routeTemCurry==datas.company[d].currency_id){
                        teamHtml+='<option value="'+datas.company[d].currency_id+'" selected>'+L('currency','currency_name',datas.company[d].currency_id,languageId,datas.company[d].currency_name)+'</option>';
