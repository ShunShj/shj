                    }
                }
                if(count==0){
                    dataList[indexs].price_currency_id='';
                    addAll.push(dataList[indexs]);
                    addAllPop.push(dataList[indexs]);
                }
            }
        })
    }
    /*if(tableId=='#teamTable'){//团队产品
