/*弹窗获取线路模板*/
function team() {
    var company_id=$("#company_id").val();
    var Mycompany_id=$("#user_company_id").val();
    var route_number=$("#route_number").val();
    var route_name=$("#route_name").val();
    $.ajax({
        type: "post",
        url: "/product/getRouteTemplateAjax",
        data : {
            company_id:company_id,
            not_company_id:Mycompany_id,
            can_watch_company_id:Mycompany_id,
            route_number:route_number,
            route_name:route_name
        },
        dataType : "json",
        success : function(result) {
            if(result.code==200){
                var addTeams=result.data;
                datas.teamplate=addTeams;
                if($("#newTeam").find("tr").length==1){
                    addTeams.isDis=1;
                }else{
                    addTeams.isDis=0;
                }
                //var tableList=$("#team_table_pop").html();团队
                var tableList=$("#template_table_pop").html();
                laytpl(tableList).render(addTeams, function(html) {
                    $("#team_table").html(html);
                    form.render();
                });
                if($("#choose_company_id").find("option").length>0){
                    layer.closeAll('loading');
                }
            }else if(result.code!=200){
                return false;
            }

        }
    })
}

/*弹窗获取资源*/
function resource() {
    //layer.load(2);
    var source_number=$("#source_number").val();
    var source_name=$("#source_name").val();
    var supplier_type_id=$("#supplier_type_id").val();
    var supplier_name=$("#supplier_name").val();

    $.ajax({
        type : "post",
        url : "/source/getSourceAjax",
        data : {
            source_number:source_number,
            source_name:source_name,
            supplier_type_id:supplier_type_id,
            supplier_name:supplier_name,
            is_branch_product:1
        },
        dataType : "json",
        success : function(result) {
            if(result.code==200){
                var addTeams=result.data;
                datas.resource=addTeams;
                var tableList=$("#product_table_pop").html();
                laytpl(tableList).render(addTeams, function(html) {
                    $("#product_tbody").html(html);
                    form.render();
                })
                //layer.closeAll('loading');
            }else if(result.code!=200){
                return false;
            }

        }
    })
}
function L(original_table_name,original_table_field_name,original_table_id,language_id,d){
    //alert(MultilingualAll[original_table_name+'-'+original_table_field_name+'-'+original_table_id+'-'+language_id][0]['translate']);
    return MultilingualAll[original_table_name+'-'+original_table_field_name+'-'+original_table_id+'-'+language_id]?MultilingualAll[original_table_name+'-'+original_table_field_name+'-'+original_table_id+'-'+language_id][0]['translate']!=''?MultilingualAll[original_table_name+'-'+original_table_field_name+'-'+original_table_id+'-'+language_id][0]['translate']:d:d;
}
//线路模板
function teamList(addAllPop,tableId) {
    if(addAllPop.length>0){
        var teamHtml='';
        for(var i=0;i<addAllPop.length;i++){
            var abcList=$(".abc tbody .firstTr").length;
            if($("#branch_product_number").val()=='' || tableId!=undefined){
                addAllPop[i].branch_customer_price=addAllPop[i].branch_cost;
                addAllPop[i].branch_distributor_price=addAllPop[i].branch_cost;
                teamHtml += '<tr class="firstTr" data-value="'+(abcList+i)+'">';
            }else{
                teamHtml+='<tr class="firstTr" data-value="'+i+'">';
            }
            teamHtml+='<td class=""><a href="/product/showRouteTemplateInfo?route_template_id='+addAllPop[i].route_template_id+'&abc=1" target="_blank" class="table_href first">'+isFieldName(addAllPop[i].route_number)+'</a></td>'+
                '<td>'+isFieldName(addAllPop[i].route_name)+'</td>'+
                '<td>'+isFieldName(addAllPop[i].route_type_name)+'</td>'+
                '<td>'+isFieldName(addAllPop[i].company_name)+'</td>';
                /*'<td>'+date('Y-m-d',addAllPop[i].begin_time)+'</td>'+
                '<td>'+isFieldName(addAllPop[i].plan_custom_number)+'</td>'+
                '<td>'+isFieldName(addAllPop[i].stance)+'</td>';*/
                if(addAllPop[i].settlement_type==1){//判断一口价或直客
                    teamHtml+='<td>'+isFieldName(addAllPop[i].unit+addAllPop[i].once_price)+'</td>';
                }else{
                    teamHtml+='<td>'+isFieldName(addAllPop[i].unit+addAllPop[i].real_price)+'</td>';
                }

            if($("#newTeam").attr("class")!=undefined){//详情页
                teamHtml+='<td class="all-select"><select class="layui-select" lay-filter="select" disabled><option value=""></option>';
            }else{
                teamHtml+='<td><select class="layui-select" lay-filter="select"><option value=""></option>';
            }

            if($("#branch_product_number").val()=='' || tableId!=undefined){
                addAllPop[i].price_currency_id=addAllPop[i].cost_currency_id;
            }

            for(var c=0;c<datas.company.length;c++){
                if(addAllPop[i].price_currency_id==datas.company[c].currency_id){
                    routeTemCurry=datas.company[c].currency_id;
                    teamHtml+='<option value="'+datas.company[c].currency_id+'" selected>'+L('currency','currency_name',datas.company[c].currency_id,languageId,datas.company[c].currency_name)+'</option>';
                }else{
                    teamHtml+='<option value="'+datas.company[c].currency_id+'">'+L('currency','currency_name',datas.company[c].currency_id,languageId,datas.company[c].currency_name)+'</option>';
                }
            }


            if($("#newTeam").attr("class")!=undefined) {//详情页
                teamHtml+='</select></td><td class="all-select"><input type="text" class="layui-input" disabled value="'+isFieldName(addAllPop[i].customer_price)+'" /></td>'+
                    '<td class="all-select"><input type="text" class="layui-input" disabled value="'+isFieldName(addAllPop[i].distributor_price)+'" /></td>';
            }else {
                if(addAllPop[i].customer_price==undefined){
                    if(addAllPop[i].settlement_type==1){
                        teamHtml+='</select></td><td><input type="text" class="layui-input" value="'+isFieldName(addAllPop[i].once_price)+'" /></td>'+
                            '<td><input type="text" class="layui-input" value="'+isFieldName(addAllPop[i].once_price)+'" /></td>';
                    }else {
                        teamHtml+='</select></td><td><input type="text" class="layui-input" value="'+addAllPop[i].real_price+'" /></td>'+
                            '<td><input type="text" class="layui-input" value="'+addAllPop[i].real_price+'" /></td>';
                    }
                }else{
                    teamHtml+='</select></td><td><input type="text" class="layui-input" value="'+isFieldName(addAllPop[i].customer_price)+'" /></td>'+
                        '<td><input type="text" class="layui-input" value="'+isFieldName(addAllPop[i].distributor_price)+'" /></td>';
                }
                teamHtml+='<td><a href="javascript:;" class="layui-btn del_source layui-btn-sm hover-edit layui-btn-primary" onclick="teamDel(this)">' + del + '</a></td></tr>';
            }
        }
        //alert(routeTemCurry)
        $("#newTeam").append(teamHtml);
        if(routeTemCurry!=''){
            $("#newProduct").find("select[lay-filter='selectProduct']").each(function (index,item) {
                $(item).val(routeTemCurry);
            })
        }
        console.log()
        form.render();
        tableNone();
        teamInput(addTeamplate);
    }
}

