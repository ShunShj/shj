//删除资源
function productDel(obj) {
    var number=$(obj).parents("tr").find(".first").html();
    $(obj).parents("tr").remove();
    for(var i=0;i<addResource.length;i++){
        if(number==addResource[i].source_number){
            addResource.splice(i,1)
        }
    }
    for(var i=0;i<addResourcePop.length;i++){
        if(number==addResourcePop[i].source_number){
            addResourcePop.splice(i,1)
        }
    }
}


function price(price,one,two) {
    return price==1?one:price==2?two:'';
}

form.on('checkbox(poperCheckboxAll)', function(data){
    var child = $(data.elem).parents('thead').siblings("tbody").find('td input[type="checkbox"]');
    child.each(function(index, item){
        item.checked = data.elem.checked;
    });
    form.render('checkbox');
});

form.on('checkbox(poperCheckboxAllTwo)', function(data){
    var child = $(data.elem).parents('thead').siblings("tbody").find('td input[type="checkbox"]');
    child.each(function(index, item){
        item.checked = data.elem.checked;
    });
    form.render('checkbox');
});
form.on('checkbox(tablePop)',function (data) {
    var teamplateOne=0;//分公司线路模板只选一个
    var child = $(data.elem).parents('tbody').find('td input[type="checkbox"]');
    child.each(function(index, item){
        if(item.checked==true){
            teamplateOne++;
        }
    });
    if(teamplateOne>1){
        data.elem.checked=false;
        alert('只能选择一个线路模板！');
    }
    form.render('checkbox');
})
/*资源类型*/
function productTypes(types) {
    return types==2?hotel:types==3?dining:types==4?flight:types==5?cruise:types==6?visa:types==7?scenic_spot:types==8?vehicle:types==9?tourguide:types==10?singlesource:types==11?ownexpense:''
}
/*判断是否是null或为空*/
function isFieldName(fieldName) {
    if(fieldName&&fieldName!=''){
        return fieldName;
    }else{
        return "";
    }
}
/*资源连接*/
function numberA(obj,ids,sourceId) {
    var html='';
    html+='<a href="/source/';
    ids==2?html+='showHotelInfo?hotel_id='+sourceId:ids==3?html+='showDiningInfo?dining_id='+sourceId:ids==4?html+='showFlightInfo?flight_id='+sourceId:ids==5?html+='showCruisesSourceInfo?id='+sourceId:ids==6?html+='showVisaSourceInfo?id='+sourceId:ids==7?html+='showScenicSpotSourceInfo?id='+sourceId:ids==8?html+='showVehicleSourceInfo?id='+sourceId:ids==9?html+='showTourGuideSourceInfo?id='+sourceId:ids==10?html+='showSingleSourceInfo?single_source_id='+sourceId:ids==11?html+='showOwnExpenseInfo?own_expense_id='+sourceId:'';
    html+='" target="_blank" class="layui-table-link">'+obj+'</a>';
    return html;
}
