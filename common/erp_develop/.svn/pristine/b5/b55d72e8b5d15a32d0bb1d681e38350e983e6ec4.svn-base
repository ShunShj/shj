@media print and (-webkit-min-device-pixel-ratio: 0) {

    /* Set backgrounp to exact color */
    body, span, div, table, tr, tbody, tfoot, td, hr {
        -webkit-print-color-adjust: exact;
    }

    @page {
        margin-left: 1cm;
        margin-right: 1cm;
        margin-top: 1.3cm;
        margin-bottom: 1.3cm;
    }

    div#notice {
        /*page-break-before: always;*/
        page-break-inside: avoid;
    }

    div#itinerary {
        /*page-break-before: always;*/
    }

    /* div#itinerary
   {
      page-break-after: always;
      page-break-inside: avoid;
     } */
    #booking_voucher_print {
        display: block;
        width: 100%;
        font-size: 16px;
    }

    #booking_voucher_print .content_img {
        max-width: 600px;
    }

    #booking_voucher #top_voucher p.left {
        margin-left: 53px;
        margin-top: 550px;
    }

    #booking_voucher_print #pax_info ul#room_info_ul {
        /*margin-top: 415px;*/
        padding-left: 10px;
    }

    #booking_voucher_print #transfer_info table#transfer_info_table {
        width: 100%;
    }

    #booking_voucher_print #accommodation_info table#accommodation_info_table {
        /*width: 950px;*/
    }

    #booking_voucher_print #accommodation_info table#accommodation_info_table tr th.date {
        /*width: 110px;*/
    }

    #booking_voucher_print #accommodation_info table#accommodation_info_table tr th.accommodation_type {
        /*width: 180px;*/
    }

    #booking_voucher_print #accommodation_info table#accommodation_info_table tr th.travellers {
        /*width: 135px;*/
    }

    #booking_voucher_print #notice table#notice_table {
        /*width: 95%;*/
        /*margin-left: 20px;*/
    }

    #booking_voucher_print #itinerary table#itinerary_table {
        /*font-size: 18px;*/
    }

}