/*收起展开*/
$(".branch-btn").click(function () {
    if($(".branch-btn").html()==='收起'){
        $(".branch-top .layui-form-item").slideUp();
        $(".branch-btn").html("展开");
    }else{
        $(".branch-top .layui-form-item").slideDown();
        $(".branch-btn").html("收起");
    }
})

