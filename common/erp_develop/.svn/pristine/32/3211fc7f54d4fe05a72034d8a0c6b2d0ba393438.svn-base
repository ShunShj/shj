            var uploadInst = upload.render({
                elem: '#journey-picture-'+i //绑定元素
                ,url: '/demo/uploadFile?day='+i, //上传接口
                multiple:'true',
                drag:'true'
                ,acceptMime:'image/*'
                ,accept:'images'
