                        }
                    }
                    ,error: function(res){
                        //请求异常回调
                        console.log(res);
                        layer.closeAll('loading'); //关闭loading
                    }
                });
            }
            sdays = i-1;
            is_days=1;
//            alert(sdays);
        });
        //行程中添加航班
        $('.d_journey').delegate('.add_transfer','click',function(){
            var day_id = $(this).data('id');
//            alert(day_id);
            var html = $('#t_transfer').html();
