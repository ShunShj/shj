.next_link {
    height: 33px;
    padding: 0;
    position: absolute;
    right: 0px;
    line-height: 33px;
}

.next_link input[type=submit] {
    font-size: 16px;
    padding: 2px 10px;
    height: 26px;
}

.back_link {
    height: 33px;
    padding: 0;
    position: absolute;
    left: 15px;
    line-height: 33px;
}


.back_link button,.next_link button {
    cursor: pointer;
    font-size: 16px;
    padding: 2px 10px;
    height: 26px;
}

#create_tour_form em {
    color: red;
}

label.admin_form_error {
    margin-left: 10px;
    width: auto;
    display: inline;
    color: red;
    font-style: italic;
}

#create_tour_form textarea.admin_form_error, #create_tour_form input.admin_form_error {
    border: 1px dotted red;
}

#admin_add_tour_general {
    width: 1000px;
}

#admin_add_tour_general section {
    margin-left: 20px;
}

#admin_add_tour_general section label.displayfield {
    display: inline-block;
    min-width: 200px;
}

#admin_add_tour_general section input[type=text] {
    width: 335px;
}

#admin_add_tour_general .checkbox_wrapper {
    display: inline-block;
    min-width: 100px;
    height: 23px;
    line-height: 23px;
    font-size: 14px;
    margin: 0px 5px 5px;
    /*vertical-align: top;*/
}

#admin_add_tour_general .checkbox_wrapper input[type=checkbox] {
    width: 17px;
    height: 17px;
    vertical-align: bottom;
}

#admin_add_tour_general .checkbox_wrapper label.admin_form_error {
    float: right;
}

.inbound_check{
    width: 16px;
    height: 16px;
    margin: 0px;
    vertical-align: bottom;
}

#tour_language_section h3{
    display: inline-block;
}

.ajax_message{
    width: 200px;
    line-height: 20px;
    padding: 5px 0;
    text-align: center;
    box-sizing: border-box;
    display: none;
    position: absolute;
    font-size: 16px;
    -webkit-box-shadow: 0 0 3px 0px rgba(0,0,0,0.5);
    box-shadow: 0 0 3px 0px rgba(0,0,0,0.5);
    overflow: hidden;
}

.success_msg {
    background-color: #9acd32;
    /*color: #4158ff;*/
}

.error_msg {
    background-color: #ff4500;
    /*color: white;*/
}

.ajax_modal {
    display: none;
    position: fixed;
    z-index: 1000;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    background: rgba(255, 255, 255, .8) url("../images/ajax-loader.gif") 50% 50% no-repeat;
}

body.ajax_loading {
    overflow: hidden;
}

body.ajax_loading .ajax_modal {
    display: block;
}



