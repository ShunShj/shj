/*login*/
.container{
    width: 340px;
    height:245px;
    position: absolute;
    right: 7%;
    top:46%;
    z-index: 999;
    margin-top:-100px;
    padding:20px 0px 30px;
    background: #fff;
    text-align: center;
    color: #666;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    -webkit-box-shadow: #666 0px 0px 25px;
    -moz-box-shadow: #666 0px 0px 25px;
    box-shadow: #666 0px 0px 25px;
}

.container h3 {
    font-size: 18px;
    -webkit-transition-duration: 1s;
    transition-duration: 1s;
    -webkit-transition-timing-function: ease-in-put;
    transition-timing-function: ease-in-put;
    font-weight: 500;
    line-height: 44px;
}
form {
    text-align: left;
    padding: 20px 0;
    position: relative;
    z-index: 2;
}
.login-pos{
    position: relative;
    width:280px;
    margin:0 auto;
}
.login-pos i{
    position: absolute;
    right:15px;
    top:0px;
    height: 48px;
    line-height: 48px;
    font-size: 22px;
}
input:-webkit-autofill {
    box-shadow: 0 0 0 1000px #fff inset !important;
}
.login-user i{
    font-size:60px;
    color:#93DEFE;
}
form input {
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    outline: 0;
    border: 1px solid #ccc;
    border-radius: 4px;
    width: 250px;
    height:48px;
    line-height: 48px;
    padding: 0px 15px;
    margin: 0 auto 15px auto;
    display: block;
    text-align: left;
    font-size: 16px;
    color: #9d9d9d;
    -webkit-transition-duration: 0.25s;
    transition-duration: 0.25s;
    font-weight: 300;
}
.login-button{
    width: 285px;
    margin: 30px auto 0px;
}
.login-forget{
    color:#666;
    font-weight: 400;
}
#login_check{
    text-align: center;
    color:#fff;
}
form .login-forget{
    width:140px;
    display: inline-block;
    cursor: pointer;
    margin:0px;
}
form #login_check{
    width: 280px;
}
form input,form input:hover,form input:focus {
    background-color:#d2d3d3;
}
form input:focus {
    /* width: 300px;*/
    color: #666;
}
#login_check{
    background: #ff5e19!important;
}
#login_check:hover{
    opacity: .8;
}